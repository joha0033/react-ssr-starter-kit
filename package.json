{
  "name": "react-ssr-starter-kit",
  "version": "1.0.0",
  "description": "React Serverside render starter kit v1",
  "main": "lib/server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name productionApp",
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node --name developmentApp",
    "test": "jest --watchAll",
    "verify-tests": "jest --coverage",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joha0033/react-ssr-starter-kit.git"
  },
  "keywords": [
    "react",
    "starter",
    "kit",
    "serverside",
    "render",
    "express"
  ],
  "author": "roam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joha0033/react-ssr-starter-kit/issues"
  },
  "homepage": "https://github.com/joha0033/react-ssr-starter-kit#readme",
  "babel": {
    "presets": [
      "react",
      ["env", { "targets": { "node": "current" } }]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.6.1",
    "enzyme-adapter-react-16": "^1.5.0",
    "express": "^4.16.3",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^3.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "webpack": "^4.19.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "enzyme": "^3.6.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.5.1",
    "webpack-cli": "^3.1.0"
  }
}
