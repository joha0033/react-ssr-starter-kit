{
  "name": "react-ssr-starter-kit",
  "version": "1.0.0",
  "description": "React Serverside render starter kit v1",
  "main": "lib/server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name papp",
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node --name dapp",
    "test": "jest -u",
    "test-watch": "npm test -- --watch",
    "verify-tests": "npm test -- --coverage",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files --ignore 'src/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joha0033/react-ssr-starter-kit.git"
  },
  "keywords": [
    "react",
    "starter",
    "kit",
    "serverside",
    "render",
    "express"
  ],
  "author": "roam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joha0033/react-ssr-starter-kit/issues"
  },
  "homepage": "https://github.com/joha0033/react-ssr-starter-kit#readme",
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "enzyme-adapter-react-16": "^1.5.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^3.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.19.0",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "clean-webpack-plugin": "^0.1.19",
    "enzyme": "^3.6.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "webpack-cli": "^3.1.0"
  }
}
